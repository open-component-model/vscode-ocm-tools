{
  "name": "vscode-ocm-tools",
  "displayName": "Open Component Model tools ",
  "description": "Open Component Mode tools for visual studio code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-ocm-tools.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-ocm-tools.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "gen:types:ocm": "tsc ./src/ocm/generateTypes.ts && node ./src/ocm/generateTypes.js && npm run clean:types:ocm",
    "clean:types:ocm": "rm ./src/ocm/generateTypes.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "js-yaml": "^4.1.0",
    "json-schema-to-typescript": "^11.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "yaml": "^2.2.1"
  }
}
